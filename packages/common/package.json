		"@graphql-tools/load": "^7.5.1",
		"@graphql-tools/load-files": "^6.5.3",
		"@graphql-tools/merge": "^8.2.1",
		"graphql": "^16.2.0"
	},
	"devDependencies": {
		"@magister_zito/eslint-config": "^0.9.2",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.5.0",
		"mongoose": "^6.1.8",
